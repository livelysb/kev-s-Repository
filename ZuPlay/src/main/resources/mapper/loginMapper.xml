<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kosta.zuplay.loginMapper">
	<select id="firstLoginCheck">
		select * from player where naver_Id=#{naverId}
	</select>

	<insert id="joinMember">
		insert into player values (#{playerNickname},#{naverId},#{)
	</insert>
</mapper>